{
  "name": "n8n-nodes-twitter-media-upload",
  "version": "0.6.0",
  "description": "n8n node for uploading media to Twitter/X",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "twitter",
    "media",
    "upload"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Deepak Dhaka",
    "email": "gxyz717@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepakdhaka-1/n8n-nodes-twitter-media-upload/"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint **/*.ts",
    "lintfix": "eslint **/*.ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TwitterMediaUploadApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TwitterMediaUpload/TwitterMediaUpload.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.8.0"
  },
  "dependencies": {
    "oauth-1.0a": "^2.2.6",
    "crypto-js": "^4.1.1",
    "puppeteer": "^21.0.0"
  }
}
